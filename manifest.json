{
  "manifest_version": 2,
  "name": "Calendar Extension",
  "version": "0.1",
  "page_action": {
    "default_title": "Google Calendar",
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon16.png",
      "32": "images/icon16.png",
      "64": "images/icon16.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ]
}
